    {
      "name": "Criação de usuário com sucesso",
      "event": [
        {
          "listen": "test",
          "script": {
            "id": "d2d20ec0-e57c-4259-97ba-e91786f2cabf",
            "exec": [
              "pm.test(\"Status code is 201. Created.\", function () {",
              "    pm.response.to.have.status(201);",
              "});",
              "",
              "pm.test(\"Campos estão corretos\", function () {",
              "    const json = pm.response.json();",
              "",
              "    pm.expect(json).to.have.property(\"id\");",
              "    pm.expect(json.id).to.be.a(\"number\");",
              "",
              "    // SALVANDO O ID PARA OUTRAS REQUISIÇÕES",
              "    pm.environment.set(\"user_id\", json.id);",
              "",
              "    pm.expect(json.name).to.eql(\"João Teste\");",
              "    pm.expect(json.email).to.eql(\"joaoTeste0@example.com\");",
              "    pm.expect(json.gender).to.eql(\"male\");",
              "    pm.expect(json.status).to.eql(\"active\");",
              "",
              "});",
              ""
            ],
            "type": "text/javascript"
